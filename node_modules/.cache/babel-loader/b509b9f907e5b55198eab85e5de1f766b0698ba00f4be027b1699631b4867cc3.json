v-avatar>\n                    </template>\n\n                    <v-list-item-title>Message {{ n }}</v-list-item-title>\n\n                    <v-list-item-subtitle>\n                      Lorem ipsum dolor sit amet, consectetur adipisicing elit. Nihil repellendus distinctio similique\n                    </v-list-item-subtitle>\n                  </v-list-item>\n\n                  <v-divider\n                    v-if=\"n !== 6\"\n                    :key=\"`divider-${n}`\"\n                    inset\n                  ></v-divider>\n                </template>\n              </v-list>\n            </v-card>\n          </v-col>\n        </v-row>\n      </v-container>\n    </v-main>\n  </v-app>\n</template>\n\n<script>\nimport Sidebar from './components/Sidebar.vue';\n\n\nexport default {\n  name: 'App',\n  components: {\n    Sidebar\n  },\n  data: () => ({\n      cards: ['Today', 'Yesterday'],\n      drawer: null,\n      links: [\n        ['mdi-inbox-arrow-down', 'Inbox'],\n        ['mdi-send', 'Send'],\n        ['mdi-delete', 'Trash'],\n        ['mdi-alert-octagon', 'Spam'],\n      ],\n    }),\n}\n</script>\n\n"],"mappings":"AAwFA,OAAOA,OAAM,MAAO,0BAA0B;AAG9C,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAI,EAAEA,CAAA,MAAO;IACTC,KAAK,EAAE,CAAC,OAAO,EAAE,WAAW,CAAC;IAC7BC,MAAM,EAAE,IAAI;IACZC,KAAK,EAAE,CACL,CAAC,sBAAsB,EAAE,OAAO,CAAC,EACjC,CAAC,UAAU,EAAE,MAAM,CAAC,EACpB,CAAC,YAAY,EAAE,OAAO,CAAC,EACvB,CAAC,mBAAmB,EAAE,MAAM,CAAC;EAEjC,CAAC;AACL"},"metadata":{},"sourceType":"module","externalDependencies":[]}
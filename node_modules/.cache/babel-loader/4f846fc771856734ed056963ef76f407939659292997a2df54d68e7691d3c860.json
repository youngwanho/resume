{"ast":null,"code":"// import Swiper core and required modules\nimport { Navigation, Pagination, Scrollbar, A11y } from 'swiper';\n\n// Import Swiper Vue.js components\nimport { Swiper, SwiperSlide } from 'swiper/vue';\n\n// Import Swiper styles\nimport 'swiper/css';\nimport 'swiper/css/navigation';\nimport 'swiper/css/pagination';\nimport 'swiper/css/scrollbar';\n\n// Import Swiper styles\nexport default {\n  components: {\n    Swiper,\n    SwiperSlide\n  },\n  data: () => ({\n    programItems: [{\n      title: 'HTML',\n      category: '퍼블리싱',\n      slogan: 'HTML을 이용한 하드코딩 및 부트스트랩 프레임워크 기반 코딩가능.'\n    }, {\n      title: 'CSS',\n      category: '퍼블리싱',\n      slogan: 'CSS를 이용한 스타일과 커스텀트릭 구현 및 SCSS를 사용한 효율적인 유지보수가능.'\n    }, {\n      title: 'JQuery',\n      category: '퍼블리싱'\n    }, {\n      title: 'JAVA Script',\n      category: '퍼블리싱'\n    }, {\n      title: 'Vue.js',\n      category: '퍼블리싱'\n    }, {\n      title: '어도비 포토샵',\n      category: '디자인'\n    }, {\n      title: '어도비 일러스트',\n      category: '디자인'\n    }]\n  }),\n  setup() {\n    const onSwiper = swiper => {\n      console.log(swiper);\n    };\n    const onSlideChange = () => {\n      console.log('slide change');\n    };\n    return {\n      onSwiper,\n      onSlideChange,\n      modules: [Navigation, Pagination, Scrollbar, A11y]\n    };\n  }\n};","map":{"version":3,"names":["Navigation","Pagination","Scrollbar","A11y","Swiper","SwiperSlide","components","data","programItems","title","category","slogan","setup","onSwiper","swiper","console","log","onSlideChange","modules"],"sources":["/Users/mac/Desktop/vue/vue-project/src/components/views/Swiper.vue"],"sourcesContent":["<template>\n  <swiper :modules=\"modules\" :slides-per-view=\"5\" :pagination=\"{ clickable: true, }\" :space-between=\"25\"\n    @swiper=\"onSwiper\" @slideChange=\"onSlideChange\">\n    <swiper-slide v-for=\"item in programItems\" :key=\"item.id\">\n      <v-sheet class=\"p-4\" height=\"150\" rounded=\"lg\">\n        <v-card>\n          <v-card-text>\n            <div>{{ item.category }}</div>\n            <p class=\"text-h6 text--primary pt-1\">{{ item.title }}</p>\n            <div class=\"text--primary\">{{item.slogan}}</div>\n          </v-card-text>\n          <v-card-actions>\n            <v-btn variant=\"text\" color=\"#5558c9\" @click=\"reveal = true\">\n              한번에 보기\n            </v-btn>\n          </v-card-actions>\n          </v-card>\n      </v-sheet>\n    </swiper-slide>\n  </swiper>\n</template>\n<script>\n// import Swiper core and required modules\nimport { Navigation, Pagination, Scrollbar, A11y } from 'swiper';\n\n// Import Swiper Vue.js components\nimport { Swiper, SwiperSlide } from 'swiper/vue';\n\n// Import Swiper styles\nimport 'swiper/css';\nimport 'swiper/css/navigation';\nimport 'swiper/css/pagination';\nimport 'swiper/css/scrollbar';\n\n// Import Swiper styles\nexport default {\n  components: {\n    Swiper,\n    SwiperSlide,\n  },\n  data: () => ({\n    programItems: [\n      { title: 'HTML', category: '퍼블리싱', slogan: 'HTML을 이용한 하드코딩 및 부트스트랩 프레임워크 기반 코딩가능.' },\n      { title: 'CSS', category: '퍼블리싱', slogan: 'CSS를 이용한 스타일과 커스텀트릭 구현 및 SCSS를 사용한 효율적인 유지보수가능.' },\n      { title: 'JQuery', category: '퍼블리싱' },\n      { title: 'JAVA Script', category: '퍼블리싱' },\n      { title: 'Vue.js', category: '퍼블리싱' },\n      { title: '어도비 포토샵', category: '디자인' },\n      { title: '어도비 일러스트', category: '디자인' },\n    ]\n  }),\n  setup() {\n    const onSwiper = (swiper) => {\n      console.log(swiper);\n    };\n    const onSlideChange = () => {\n      console.log('slide change');\n    };\n    return {\n      onSwiper,\n      onSlideChange,\n      modules: [Navigation, Pagination, Scrollbar, A11y],\n    };\n  },\n};\n</script>"],"mappings":"AAsBA;AACA,SAASA,UAAU,EAAEC,UAAU,EAAEC,SAAS,EAAEC,IAAG,QAAS,QAAQ;;AAEhE;AACA,SAASC,MAAM,EAAEC,WAAU,QAAS,YAAY;;AAEhD;AACA,OAAO,YAAY;AACnB,OAAO,uBAAuB;AAC9B,OAAO,uBAAuB;AAC9B,OAAO,sBAAsB;;AAE7B;AACA,eAAe;EACbC,UAAU,EAAE;IACVF,MAAM;IACNC;EACF,CAAC;EACDE,
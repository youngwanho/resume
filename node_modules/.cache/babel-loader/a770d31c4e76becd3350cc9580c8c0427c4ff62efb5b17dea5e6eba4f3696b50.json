{"ast":null,"code":"// import Swiper core and required modules\nimport { Navigation, Pagination, Scrollbar, A11y } from 'swiper';\n\n// Import Swiper Vue.js components\nimport { Swiper, SwiperSlide } from 'swiper/vue';\n\n// Import Swiper styles\nimport 'swiper/css';\nimport 'swiper/css/navigation';\nimport 'swiper/css/pagination';\nimport 'swiper/css/scrollbar';\n\n// Import Swiper styles\nexport default {\n  components: {\n    Swiper,\n    SwiperSlide\n  },\n  data: () => ({\n    programItems: [{\n      title: 'HTML',\n      category: '퍼블리싱',\n      slogan: 'HTML을 이용한 하드코딩 및 부트스트랩 프레임워크 기반 코딩가능.'\n    }, {\n      title: 'CSS',\n      category: '퍼블리싱',\n      slogan: 'CSS를 이용한 커스텀,애니메이션,포커싱,반응형 및 SCSS를 이용한 효율적인 유지보수 가능.'\n    }, {\n      title: 'JQuery',\n      category: '퍼블리싱',\n      slogan: 'JQuery를 이용한 이벤트 구현 및 다양한 라이브러리 사용가능.'\n    }, {\n      title: 'JAVA Script',\n      category: '퍼블리싱',\n      slogan: 'JAVA Script를 이용한 이벤트 구현 및 다양한 라이브러리 사용가능.'\n    }, {\n      title: 'Vue.js',\n      category: '퍼블리싱',\n      slogan: 'Vue.js프레임워크를 사용한 퍼블리싱 가능 및 다양한 라이브러리 사용가능.'\n    }, {\n      title: '어도비 포토샵',\n      category: '디자인',\n      slogan: '포토샵의 기본적인 툴사용 및 편집디자인 가능.'\n    }, {\n      title: '어도비 일러스트',\n      category: '디자인',\n      slogan: '일러스트의 기본적인 툴사용 및 편집디자인 가능.'\n    }, {\n      title: '어도비 XD',\n      category: '디자인',\n      slogan: 'XD의 기본적인 툴사용 및 UI/UX디자인 가능.'\n    }]\n  }),\n  setup() {\n    const onSwiper = swiper => {\n      console.log(swiper);\n    };\n    const onSlideChange = () => {\n      console.log('slide change');\n    };\n    return {\n      onSwiper,\n      onSlideChange,\n      modules: [Navigation, Pagination, Scrollbar, A11y]\n    };\n  }\n};","map":{"version":3,"names":["Navigation","Pagination","Scrollbar","A11y","Swiper","SwiperSlide","components","data","programItems","title","category","slogan","setup","onSwiper","swiper","console","log","onSlideChange","modules"],"sources":["/Users/mac/Desktop/vue/vue-project/src/components/views/Swiper.vue"],"sourcesContent":["<template>\n  <swiper :modules=\"modules\" :slides-per-view=\"5\" :pagination=\"{ clickable: true, }\" :space-between=\"25\"\n    @swiper=\"onSwiper\" @slideChange=\"onSlideChange\">\n    <swiper-slide v-for=\"item in programItems\" :key=\"item.id\">\n      <v-sheet class=\"p-4\" height=\"150\" rounded=\"lg\">\n        <v-card height=\"150\">\n          <v-card-text>\n            <div>\n              <div>{{ item.category }}</div>\n              <p class=\"text-h6 text--primary pt-1\">{{ item.title }}</p>\n            </div>            \n          </v-card-text>\n          <div class=\"text--primary\">{{item.slogan}}</div>        \n          </v-card>\n      </v-sheet>\n    </swiper-slide>\n  </swiper>\n</template>\n<script>\n// import Swiper core and required modules\nimport { Navigation, Pagination, Scrollbar, A11y } from 'swiper';\n\n// Import Swiper Vue.js components\nimport { Swiper, SwiperSlide } from 'swiper/vue';\n\n// Import Swiper styles\nimport 'swiper/css';\nimport 'swiper/css/navigation';\nimport 'swiper/css/pagination';\nimport 'swiper/css/scrollbar';\n\n// Import Swiper styles\nexport default {\n  components: {\n    Swiper,\n    SwiperSlide,\n  },\n  data: () => ({\n    programItems: [\n      { title: 'HTML', category: '퍼블리싱', slogan: 'HTML을 이용한 하드코딩 및 부트스트랩 프레임워크 기반 코딩가능.' },\n      { title: 'CSS', category: '퍼블리싱', slogan: 'CSS를 이용한 커스텀,애니메이션,포커싱,반응형 및 SCSS를 이용한 효율적인 유지보수 가능.' },\n      { title: 'JQuery', category: '퍼블리싱', slogan: 'JQuery를 이용한 이벤트 구현 및 다양한 라이브러리 사용가능.' },\n      { title: 'JAVA Script', category: '퍼블리싱', slogan: 'JAVA Script를 이용한 이벤트 구현 및 다양한 라이브러리 사용가능.'  },\n      { title: 'Vue.js', category: '퍼블리싱', slogan: 'Vue.js프레임워크를 사용한 퍼블리싱 가능 및 다양한 라이브러리 사용가능.' },\n      { title: '어도비 포토샵', category: '디자인', slogan: '포토샵의 기본적인 툴사용 및 편집디자인 가능.' },\n      { title: '어도비 일러스트', category: '디자인', slogan: '일러스트의 기본적인 툴사용 및 편집디자인 가능.' },\n      { title: '어도비 XD', category: '디자인', slogan: 'XD의 기본적인 툴사용 및 UI/UX디자인 가능.' },\n    ]\n  }),\n  setup() {\n    const onSwiper = (swiper) => {\n      console.log(swiper);\n    };\n    const onSlideChange = () => {\n      console.log('slide change');\n    };\n    return {\n      onSwiper,\n      onSlideChange,\n      modules: [Navigation, Pagination, Scrollbar, A11y],\n    };\n  },\n};\n</script>"],"mappings":"AAmBA;AACA,SAASA,UAAU,EAAEC,UAAU,EAAEC,SAAS,EAAEC,IAAG,QAAS,QAAQ;;AAEhE;AACA,SAASC,MAAM,EAAEC,WAAU,QAAS,YAAY;;AAEhD;AACA,OAAO,YAAY;AACnB,OAAO,uBAAuB;AAC9B,OAAO,uBAAuB;AAC9B,OAAO,sBAAsB;;AAE7B;AACA,eAAe;EACbC,UAAU,EAAE;IACVF,MAAM;IACNC;EACF,CAAC;EACDE,IAAI,EAAEA,CAAA,MAAO;IACXC,YAAY,EAAE,CACZ;MAAEC,KAAK,EAAE,MAAM;MAAEC,QAAQ,EAAE,MAAM;MAAEC,MAAM,EAAE;IAAwC,CAAC,EACpF;MAAEF,KAAK,EAAE,KAAK;MAAEC,QAAQ,EAAE,MAAM;MAAEC,MAAM,EAAE;IAAuD,CAAC,EAClG;MAAEF,KAAK,EAAE,QAAQ;MAAEC,QAAQ,EAAE,MAAM;MAAEC,MAAM,EAAE;IAAuC,CAAC,EACrF;MAAEF,KAAK,EAAE,aAAa;MAAEC,QAAQ,EAAE,MAAM;MAAEC,MAAM,EAAE;IAA6C,CAAC,EAChG;MAAEF,KAAK,EAAE,QAAQ;MAAEC,QAAQ,EAAE,MAAM;MAAEC,MAAM,EAAE;IAA6C,CAAC,EAC3F;MAAEF,KAAK,EAAE,SAAS;MAAEC,QAAQ,EAAE,KAAK;MAAEC,MAAM,EAAE;IAA4B,CAAC,EAC1E;MAAEF,KAAK,EAAE,UAAU;MAAEC,QAAQ,EAAE,KAAK;MAAEC,MAAM,EAAE;IAA6B,CAAC,EAC5E;MAAEF,KAAK,EAAE,QAAQ;MAAEC,QAAQ,EAAE,KAAK;MAAEC,MAAM,EAAE;IAA8B,CAAC;EAE/E,CAAC,CAAC;EACFC,KAAKA,CAAA,EAAG;IACN,MAAMC,QAAO,GAAKC,MAAM,IAAK;MAC3BC,OAAO,CAACC,GAAG,CAACF,MAAM,CAAC;IACrB,CAAC;IACD,MAAMG,aAAY,GAAIA,CAAA,KAAM;MAC1BF,OAAO,CAACC,GAAG,CAAC,cAAc,CAAC;IAC7B,CAAC;IACD,OAAO;MACLH,QAAQ;MACRI,aAAa;MACbC,OAAO,EAAE,CAAClB,UAAU,EAAEC,UAAU,EAAEC,SAAS,EAAEC,IAAI;IACnD,CAAC;EACH;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}
{"ast":null,"code":"// import Swiper core and required modules\nimport { Navigation, Pagination, Scrollbar, A11y } from 'swiper';\n\n// Import Swiper Vue.js components\nimport { Swiper, SwiperSlide } from 'swiper/vue';\n\n// Import Swiper styles\nimport 'swiper/css';\nimport 'swiper/css/navigation';\nimport 'swiper/css/pagination';\nimport 'swiper/css/scrollbar';\n\n// Import Swiper styles\nexport default {\n  components: {\n    Swiper,\n    SwiperSlide\n  },\n  data: () => ({\n    programItems: [{\n      title: 'HTML',\n      category: '개발'\n    }, {\n      title: 'CSS',\n      catrgory: '개발'\n    }, {\n      title: 'GTQ(포토샵) 1급',\n      date: '2022년 04월 12일'\n    }, {\n      title: 'GTQI(일러스트) 1급',\n      date: '2022년 04월 12일'\n    }, {\n      title: 'JLPT(일본어) 3급',\n      date: '2022년 04월 12일'\n    }, {\n      title: '조경기능사',\n      date: '2022년 04월 12일'\n    }, {\n      title: '레크레이션강사',\n      date: '2022년 04월 12일'\n    }, {\n      title: '웃음치료사',\n      date: '2022년 04월 12일'\n    }, {\n      title: '펀리더쉽지도자',\n      date: '2022년 04월 12일'\n    }, {\n      title: '운전면허증 1종',\n      date: '2022년 04월 12일'\n    }]\n  }),\n  setup() {\n    const onSwiper = swiper => {\n      console.log(swiper);\n    };\n    const onSlideChange = () => {\n      console.log('slide change');\n    };\n    return {\n      onSwiper,\n      onSlideChange,\n      modules: [Navigation, Pagination, Scrollbar, A11y]\n    };\n  }\n};","map":{"version":3,"names":["Navigation","Pagination","Scrollbar","A11y","Swiper","SwiperSlide","components","data","programItems","title","category","catrgory","date","setup","onSwiper","swiper","console","log","onSlideChange","modules"],"sources":["/Users/mac/Desktop/vue/vue-project/src/components/views/Swiper.vue"],"sourcesContent":["<template>\n  <swiper :modules=\"modules\" :slides-per-view=\"5\" :pagination=\"{ clickable: true, }\" :navigation=\"true\" :space-between=\"25\"\n    @swiper=\"onSwiper\" @slideChange=\"onSlideChange\">\n    <swiper-slide v-for=\